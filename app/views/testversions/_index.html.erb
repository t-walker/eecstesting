<table class="table" style="color:black;">
  <tr>
    <th>Title</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Active?</th>
  </tr>
<% testversions.each do |tv| %>
  <tr>
    <td><%= tv.title %></td>
    <td><%= tv.startdate %></td>
    <td><%= tv.enddate %></td>
    <td><%= tv.is_open? ? "True" : "False" %></td>
  </tr>
  <% end %>
</table>
<%= link_to "New Version", new_testversion_path %>

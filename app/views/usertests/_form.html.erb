<div class="col-md-8">
<%= form_for(@usertest) do |f| %>
  <% @testversion.questions.each do |q| %>
    <%= f.fields_for :responses, :url => new_usertest_path(@f) do |r| %>
      <%= r.hidden_field :question_id, :value => q.id %>
      <%= render 'responses/form', question: q, response: r %>
    <% end %>
    <br />
  <% end %>
  <%= f.submit "Submit Test", class: "btn btn-info" %>
<% end %>
</div>
